{
    "name": "Tatool Grid",
    "author": "Andre Locher",
    "label": "tatoolGrid",
    "projectUrl": "http://www.tatool.ch/tatoolweb/projects/demo/",
    "fullscreen": false,
    "allowEscapeKey": true,
    "moduleHierarchy": {
    	"tatoolType": "List",
        "iterator": { "customType" : "ListIterator", "numIterations" : "3" },
        "handlers": [ { "customType" : "levelHandler", "name" : "level" } ],
    	"children": 
    		[
                {
                    "tatoolType": "Executable",
                    "customType": "tatoolGridExecutable",
                    "name": "tatoolGrid",
                    "blankInterval" : "250"
                }
    		]
    }
}