                                    "levelPanel": true,
                                    "feedbackPanel": true,
                                    "trialCountPanel": true,
                                    "timerPanel": true
                                }
                            }, 
                            "secondary" : {
                                "tatoolType": "Executable",
                                "customType": "tatoolStroop",
