{
    "name": "DB Regression",
    "id": "dbRegression",
    "author": "Tatool",
    "version": 3,
    "packagePath": "packages/test",
    "allowEscapeKey" : true,
    "export" : [ 
        { "mode": "local", "auto": false, "target": "uploads/abc/" }, 
        { "mode": "remote", "auto": false, "target": "http://www.tatool.ch/tatoolweb/upload.php" },
        { "mode": "download" } ],
    "moduleHierarchy": {
    	"tatoolType": "List",
        "iterator": { "customType" : "ListIterator", "numIterations" : "1000" },
        "handlers": [ ],
    	"children": 
    		[
                {
                    "tatoolType": "Executable",
                    "customType": "dbRegression",
                    "name": "regressionTask",
                    "blankInterval" : "10"
                }
    		]
    }
}